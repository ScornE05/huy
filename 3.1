#include <stdio.h>
int main(){
	printf("Nhap vao diem cua sinh vien: ");
	float a; 
	scanf("%f", &a);
	if( 0 <= a && a <= 10  ){
		if(a < 3.5 ){
			printf("Sinh vien dat hoc luc kem! ");
		} else if( 3.5 <= a && a < 5 ){
			printf("Sinh vien dat hoc luc yeu! ");
		} else if (5 <= a && a < 6.5){
			printf("Sinh vien dat hoc luc trung binh! ");
		} else if (6.5 <= a && a < 8 ){
			printf("Sinh vien dat hoc luc kha! ");
		} else if (8 <= a && a < 9){
			printf("Sinh vien dat hoc luc gioi!");
		} else if ( a >= 9){
			printf("Sinh vien dat hoc luc xuat sac!");
		}
	} else {
		printf("Diem nhap vao khong hop le!");
	}
	return 0;                       
}
