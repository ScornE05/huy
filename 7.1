#include<stdio.h>

int main(){
	char chuoi[100];
	printf("Nhap chuoi: ");
	gets(chuoi);
	int i = 0;
	int nguyenAm = 0;
	int phuAm = 0;
	while (chuoi[i] != '\0'){
		if ( chuoi[i] == 'a' || chuoi[i] == 'u' || chuoi[i] == 'e' || chuoi[i] == 'o' || chuoi[i] == 'i' ){
			phuAm++;
		} else {
			nguyenAm++;
		}
		i++;
	}
	printf("Chuoi ban nhap :'%s' co '%d' phu am va co '%d' nguyen am. ",chuoi,phuAm,nguyenAm);
	return 0;
}
