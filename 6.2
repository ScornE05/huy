#include <stdio.h>

int main(){
	int n;
	printf("Nhap vao so phan tu ban muon co trong mang: ");
	scanf("%d", &n);
	int a[n];
	int i, max;
	for (i = 0; i < n; i++){
		printf("Nhap vao vi tri thu %d: ", i);
		scanf("%d", &a[i]);
	}
	max = a[0];
	for (i = 1; i < n; i++) {
		if (a[i] > max) {
			max = a[i];
		}
	}
	printf("So lon nhat trong mang la: %d", max);
	return 0;
}
